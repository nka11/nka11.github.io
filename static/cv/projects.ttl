        [
            a elm:LearningOutcome, schema:DefinedTerm ;
            elm:label "DevOps practices" ;
            elm:relatedESCOSkill <https://data.europa.eu/esco/skill/9c7e6502-4572-4bd3-bd15-771019a92858>
