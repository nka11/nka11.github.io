nka:me a schema:Person, schema:CreativeWork ;
    schema:name "Nicolas Karageuzian" ;
    schema:email "mailto:nkarageuzian@gmail.com" ;
    schema:url <http://github.com/nka11> ;
    schema:jobTitle
        # "Full Cycle Engineer/General IT Specialist"@en ,
        "Ingénieur Logiciel Cycle Complet - Specialiste en Informatique Génerale", #@fr ,
        # "Conseil en Cyber-Sécurité spécialiste des Architecture"@fr ,
        [ 
            a schema:DefinedTerm ;
            schema:name "Expert en Cybersécurité" ;
