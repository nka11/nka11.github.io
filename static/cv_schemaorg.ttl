        schema:name "Club Méditerranée SA"
    ] ;
    schema:hasCredential [
            a schema:EducationalOccupationalCredential ;
            schema:credentialCategory "Experience" ;
            schema:name "Architecte Technique" ;
            schema:validFor "P2Y"^^xsd:duration ;
            schema:startDate "2001-06-01"^^xsd:date ;
            schema:endDate "2005-06-01"^^xsd:date ;
            schema:subjectOf
                [
                    a schema:Project ;
                    schema:name "Horizon Centre d'Appels" ;
                    schema:description """
                        Conception de l'architecture technique et logicielle d'un couplage téléphonie/informatique
                        Spécifications technique détaillée
                        Packaging, Déploiement et suivi d'exploitation.
                    """ ;
                    schema:about
                        [
                            a elm:LearningOutcome ;
                            elm:targetFramework "ESCO" ;
                            elm:targetName "Full Stack Development" ;
                            elm:targetResource <https://data.europa.eu/esco/occupation/4031e2fa-31c8-4b3f-bc44-84e5a0aef879>
                        ],
                        [
                            a elm:LearningOutcome ;
                            elm:targetName "DevOps practices" ;
                            elm:targetResource <https://data.europa.eu/esco/skill/9c7e6502-4572-4bd3-bd15-771019a92858>
                        ],
                        [
                            a elm:LearningOutcome ;
                            elm:targetName "Gestion de projet Agile" ;
                            elm:targetResource <https://data.europa.eu/esco/skill/e5b94c9d-12ef-4cc6-81ad-6602c207a3e1>
                        ]
                ],
                [
                    a schema:Project ;
                    schema:name "Controle qualité, packaging et deploiement J2EE" ;
                    schema:description """
                        Rôle pivot entre les équipes de dev et d'exploitation.
                        - Preparation des livraisons
                        - Assistance au debugging
                    """ ;
                    schema:about
                        [
                            a elm:LearningOutcome ;
                            elm:targetName "DevOps practices" ;
                            elm:targetResource <https://data.europa.eu/esco/skill/9c7e6502-4572-4bd3-bd15-771019a92858>
                        ],
                        [
