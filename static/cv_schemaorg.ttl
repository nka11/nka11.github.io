@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix elm: <https://data.europa.eu/snb/elm/> .
@prefix esco: <https://data.europa.eu/esco/skill/> .
@prefix ex: <http://nka11.github.io/cv#> .

ex:nkarageuzian a schema:Person ;
    schema:name "Nicolas Karageuzian" ;
    schema:email "mailto:nkarageuzian@example.com" ;
    schema:url <http://github.com/nka11> ;
    schema:jobTitle "Senior Security Architect, DevOps & Tech Lead" ;
    schema:birthPlace [
        a schema:Place ;
        schema:address [
            a schema:Country ;
            schema:name "France"
        ]
    ] ;
    schema:knowsLanguage "fr", "en" ;
    schema:hasSkill "Java", "Docker", "SPARQL", "Azure" ;

    schema:alumniOf ex:edu1, ex:edu2 ;
    schema:hasOccupation ex:exp1, ex:exp2, ex:exp3, ex:exp4 ;
    schema:hasPart ex:proj1, ex:proj2 .

### Formations

ex:edu1 a schema:EducationalOccupationalCredential ;
    schema:name "DUT Informatique" ;
    schema:educationalLevel "Diplôme universitaire de technologie" ;
    schema:awardingBody [
        a schema:EducationalOrganization ;
        schema:name "IUT de Fontainebleau"
    ] ;
    schema:startDate "1996-01-01"^^xsd:date ;
    schema:endDate "1998-12-31"^^xsd:date .

ex:edu2 a schema:EducationalOccupationalCredential ;
    schema:name "Bac S option Technologie" ;
    schema:educationalLevel "Baccalauréat Scientifique" ;
    schema:awardingBody [
        a schema:EducationalOrganization ;
        schema:name "Lycée George Sand"
    ] ;
    schema:endDate "1996-06-30"^^xsd:date .

