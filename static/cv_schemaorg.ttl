                            elm:targetName "Gestion de projet Agile" ;
                            elm:targetResource <https://data.europa.eu/esco/skill/e5b94c9d-12ef-4cc6-81ad-6602c207a3e1>
                        ]
                ]

    ].

ex:exp3 a schema:OrganizationRole ;
    schema:roleName "Responsable Sécurité du SI" ;
    schema:startDate "2024-06-01"^^xsd:date ;
    schema:endDate "2025-06-30"^^xsd:date ;
